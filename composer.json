{
    "name": "butcherman/tech_bench",
    "description": "A custom CMS built on the Laravel Framework",
    "keywords": ["CMS"],
    "license": "GNU",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "doctrine/dbal": "^2.7",
        "enyo/dropzone": "^5.5",
        "fideloper/proxy": "^4.0",
        "jackiedo/timezonelist": "^5.0",
        "jeroendesloovere/vcard": "^1.6",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "pion/laravel-chunk-upload": "^1.2",
        "pragmarx/version": "^0.2.8",
        "rap2hpoutre/laravel-log-viewer": "^0.22.1",
        "tinymce/tinymce": "^4.9",
        "zanysoft/laravel-zip": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
		"post-install-cmd": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
