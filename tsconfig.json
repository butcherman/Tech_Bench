{
    "compilerOptions": {
        "jsx": "preserve",
        "baseUrl": ".",
        "paths": {
            "@/*": ["resources/js/*"]
        },
        "experimentalDecorators": false,
        "target": "ES2020",
        "moduleResolution": "node",
        "module": "ES2020",
        "strict": true,
        "types": ["vite/client"],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "importHelpers": true,
        "typeRoots": ["./resources/js/Types/", "./node_modules/@types"],
        "allowUnreachableCode": false,
        "esModuleInterop": true
    }
}
